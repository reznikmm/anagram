Token Id;
Token Plus;
Token Star;

E  ::= T E1;
E1 ::= [ Plus T E1 ] ;
T  ::= F T1;
T1 ::= [ Star F T1 ] ;
F  ::= Id;

Inherited attributes
.* : Aaa : env;

Synthesized attributes
.* : Bbb : ast;

Rules for E. :
(. ${E.ast} := ${T.ast} + ${E1.ast}; .)

Rules for E. :
(. ${T.env} := ${E.env}; .)

Rules for E. :
(. ${E1.env} := ${E.env}; .)

Rules for F. :
(. ${F.ast} := 0; .)
